
node {
    
    
    
   agent any
    
   tools{
        
         dockerTool 'docker'
    }

    stage('Clone repository') {
        /* Cloning the Repository to our Workspace */

        checkout scm
       }
    
    stage("Build image") {
            steps {
                script {
                    myapp = docker.build("mdshafi/nodejs:${env.BUILD_ID}")
                }
            }
        }
        


  
   
}
